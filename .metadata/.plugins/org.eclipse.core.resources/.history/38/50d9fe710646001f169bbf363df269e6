package entities;

public class Individual extends Servidor {

	private Double healthExpenditure;
	
	public Individual() {
		super();
	}
	
	public Individual(String name, Double income, Double healthExpenditure) {
		super(name, income);
		this.healthExpenditure = healthExpenditure;
	}

	@Override
	public Double tax() {
		return income;
	}

}
